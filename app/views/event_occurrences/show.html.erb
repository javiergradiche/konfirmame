<%- model_class = EventOccurrence -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:event_id) %>:</strong></dt>
  <dd><%= @event_occurrence.event_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:start_datetime) %>:</strong></dt>
  <dd><%= @event_occurrence.start_datetime %></dd>
  <dt><strong><%= model_class.human_attribute_name(:state) %>:</strong></dt>
  <dd><%= @event_occurrence.state %></dd>
  <dt><strong><%= model_class.human_attribute_name(:aforo) %>:</strong></dt>
  <dd><%= @event_occurrence.aforo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:num_confirm) %>:</strong></dt>
  <dd><%= @event_occurrence.num_confirm %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              event_occurrences_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_event_occurrence_path(@event_occurrence), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              event_occurrence_path(@event_occurrence),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
