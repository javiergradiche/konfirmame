<%= form_for @event_occurrence, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :event_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :event_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :start_datetime, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :start_datetime, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :state, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :state, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :aforo, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :aforo, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :num_confirm, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :num_confirm, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                event_occurrences_path, :class => 'btn' %>
  </div>
<% end %>
