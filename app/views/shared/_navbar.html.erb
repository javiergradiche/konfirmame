<%#= nav_bar :fixed => :top, :brand => "Konfirmame", :responsive => true do %>
<div class="navbar navbar-default navbar-static-top">
  <div class="container">
    <%= menu_group do %>
        <%= menu_item "Home", root_path %>
        <%= menu_divider %>
        <%= drop_down "Events" do %>
            <%= menu_item "Events", events_path %>
            <%= menu_item "Occurrences", event_occurrences_path %>
            <%= menu_item "Users", user_events_path %>
        <% end %>
        <%= menu_item "Users", user_events_path %>
        <%= menu_item "Notificiations", notifications_path %>
    <% end %>
    <%= menu_group :pull => :right do %>
        <% if user_signed_in? %>
            <%= link_to "Log out", destroy_user_session_path, :method => :delete %>
        <% end %>
    <% end %>
<%# end %>
  </div>
</div>

